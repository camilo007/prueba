////////////////////////////////////Estilos de las tablas/////////////////////////////////////////////////


.color-t
  background: #f8f8f8!important
  border: 1px solid #fd7800;



.Color-resultados
  color black!important


.Table-container-maletines
  width: 100%
  &-elements
    padding: .7em 1em
    text-align: left


@media only screen and (max-width: 760px), (min-width: 760px) and (max-width: 1024px){
  table.Table-container-maletines, .Table-container-maletines thead, .Table-container-maletines tbody, .Table-container-maletines th, .Table-container-maletines td, .Table-container-maletines tr {
    display: block; }

  .Table-container-maletines thead tr {
    position: absolute;
    display: none
  }

  .color-t{
    border: none
  }

  .Table-container-maletines tr {
    margin-bottom: 2em;
    border: 1px solid blue!important; } //color del borde en la medida mas pequeña
  .Table-container-maletines td {
    position: relative;
    padding-left: 35%; //mover la letra del correo
    font-size: 12px;

    text-align:left }
  .Table-container-maletines td:before {
    position: absolute;
    top: 0px; left: -1px;
    width: 34%; //ancho de lo sapote
    white-space: nowrap
    display: flex;
    align-items: center;
    color: #FFF
    height: 100%; //ancho hacia bajo
    padding: 0em 2% //la letra
    background-color: blue;} //color del encabezado en la medida mas pequeña

.Table-container-maletines td:nth-of-type(1):before { content: "Id Maletin"; }
.Table-container-maletines td:nth-of-type(2):before { content: "Referencia"; }
.Table-container-maletines td:nth-of-type(3):before { content: "Bolsillos"; }
.Table-container-maletines td:nth-of-type(4):before { content: "Material"; }
.Table-container-maletines td:nth-of-type(5):before { content: "Impermeable"; }
.Table-container-maletines td:nth-of-type(6):before { content: "Color"; }
.Table-container-maletines td:nth-of-type(7):before { content: "Id Marca"; }
.Table-container-maletines td:nth-of-type(8):before { content: "Editar"; }
.Table-container-maletines td:nth-of-type(9):before { content: "Eliminar"; }

