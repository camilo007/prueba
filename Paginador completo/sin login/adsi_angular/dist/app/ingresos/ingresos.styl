.Paginador {
  height: 6em !important;
  display: flex;
  width: 10em;
  padding: 0;
  margin: auto;
  justify-content: space-between;
  position: relative;
  border-top: none;
  color: #3c948b !important;
}
.Paginador >.label {
  display: none;
  position: absolute;
}
.Paginador >md-select {
  position: absolute;
  left: 50%;
  top: 25px;
  transform: translateX(-66%);
}
